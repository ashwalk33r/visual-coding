{
  "name": "visual-coding",
  "version": "1.0.0",
  "main": "index.js",
  "sideEffects": false,
  "repository": "https://github.com/ashwalk33r/visual-coding.git",
  "license": "MIT",
  "scripts": {
    "prettier": "prettier --check \"./**/*.(js|ts|json|html|scss)\"",
    "prettier:fix": "npm run prettier -- --write",
    "start": "yarn build serve",
    "serve": "node ./node_modules/http-server/bin/http-server ./dist -d false -c-1 --no-dotfiles",
    "build": "npx webpack",
    "build:watch": "yarn build --watch",
    "analyze": "webpack --profile --json > ./stats.json && npx webpack-bundle-analyzer ./stats.json"
  },
  "dependencies": {
    "d3": "^6.6.2",
    "d3-scale-chromatic": "^2.0.0",
    "extra-math": "^1.1.18",
    "jquery": "^3.6.0",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@types/d3": "^6.3.0",
    "@types/jquery": "^3.5.5",
    "@types/lodash": "^4.14.168",
    "@types/p5": "^0.9.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.2",
    "html-webpack-plugin": "^5.1.0",
    "html5-boilerplate": "^8.0.0",
    "http-server": "^0.12.3",
    "p5": "^1.3.1",
    "prettier": "^2.2.1",
    "sass": "^1.32.7",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.5",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
